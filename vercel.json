{
    "version": 1,
    "builds": [
      { "src": "requirements.txt", "use": "@vercel/python" },
      { "src": "blogsite/wsgi.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/media/(.*)", "dest": "/media/$1" },
      { "src": "/(.*)", "dest": "blogsite/wsgi.py" }
    ]
  }
  